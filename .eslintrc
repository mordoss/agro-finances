{
   "parserOptions": {
    "ecmaVersion": 2020
  },
  "extends": ["airbnb", "prettier", "prettier/react"],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "singleQuote": true,
        "semi": true
      }
    ],
    "import/no-named-as-default": 0,
    "no-use-before-define": [
      "error",
      { "functions": true, "classes": true, "variables": false }
    ],
    "react/prop-types": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ]
  },
  "plugins": ["prettier"]
}
